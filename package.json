{
  "name": "gomap",
  "version": "1.0.0",
  "description": "goeland map with openlayers 3 custom build",
  "main": "index.js",
  "scripts": {
    "build_webpack_dist": "rimraf dist && NODE_ENV=production webpack",
    "clean_dist": "rimraf dist",
    "deploy_2_vps": "rsync -rlptDv ./dist/ myvps:/var/www/gomap_gil_town/ && rsync -rlptDv ./examples/data myvps:/var/www/gomap_gil_town/",
    "dev_webpack_server": "NODE_ENV=development node dev-server.js",
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lao-tseu-is-alive/gomap.git"
  },
  "keywords": [
    "goeland",
    "openlayers",
    "ol3",
    "clojure"
  ],
  "author": "cgil",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/lao-tseu-is-alive/gomap/issues"
  },
  "homepage": "https://github.com/lao-tseu-is-alive/gomap#readme",
  "dependencies": {
    "jquery": "^3.1.1",
    "ol3-layerswitcher": "^1.1.2",
    "openlayers": "^3.20.1",
    "skeleton-plus": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "browserify": "^14.0.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.5.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "file-loader": "^0.10.0",
    "google-closure-library": "^20170124.0.0",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.28.0",
    "lite-server": "^2.2.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.2.1"
  },
  "jshintConfig": {
    "maxerr": 50,
    "esversion": 6
  }
}
